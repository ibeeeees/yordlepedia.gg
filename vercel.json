{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "website.html",
      "use": "@vercel/static"
    },
    {
      "src": "privacy.html",
      "use": "@vercel/static"
    },
    {
      "src": "tos.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/privacy",
      "dest": "privacy.html"
    },
    {
      "src": "/tos",
      "dest": "tos.html"
    },
    {
      "src": "/(.*)",
      "dest": "website.html"
    }
  ],
  "env": {
    "RIOT_API_KEY": "@riot_api_key",
    "AWS_REGION": "@aws_region",
    "COGNITO_USER_POOL_ID": "@cognito_user_pool_id",
    "COGNITO_CLIENT_ID": "@cognito_client_id",
    "COGNITO_CLIENT_SECRET": "@cognito_client_secret",
    "COGNITO_DOMAIN": "@cognito_domain",
    "RIOT_CLIENT_ID": "@riot_client_id",
    "RIOT_CLIENT_SECRET": "@riot_client_secret",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_NAME": "@db_name"
  }
}
