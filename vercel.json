{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate, max-age=0",
        "Pragma": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
